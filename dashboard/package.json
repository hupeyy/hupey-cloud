{
	"name": "dashboard",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently --names \"FRONTEND,BACKEND\" --prefix-colors \"blue,green\" \"npm run frontend\" \"npm run backend\"",
		"build": "vite build",
		"frontend": "vite dev",
		"backend": "cd backend && ./venv/bin/python -m fastapi dev main.py",
		"update-backend-deps": "cd backend && ./venv/bin/pip install -r req.txt",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"tailwindcss": "^4.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"concurrently": "^6.5.1"
	}
}
